{
	"name": "with-typescript",
	"version": "1.0.0",
	"scripts": {
		"dev": "next",
		"build": "next build",
		"start": "next start",
		"dev:nexus": "env-cmd -f ./prisma/.env nexus dev",
		"migrate:save": "prisma migrate save --experimental",
