		"nexus": "^0.25.0",
		"nexus-plugin-jwt-auth": "^1.2.0",
		"nexus-plugin-paljs": "^1.1.8",
		"nexus-plugin-prisma": "^0.16.1",
		"react": "^16.13.1",
		"react-dom": "^16.13.1"
	},
	"devDependencies": {
		"@types/bcryptjs": "^2.4.2",
		"@types/jsonwebtoken": "^8.5.0",
