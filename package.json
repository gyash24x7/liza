{
	"name": "with-typescript",
	"version": "1.0.0",
	"scripts": {
		"dev": "env-cmd -f ./prisma/.env next",
		"build": "next build",
		"start": "next start",
		"dev:nexus": "env-cmd -f ./prisma/.env nexus dev",
		"nexus:reflection": "nexus dev --reflection --entrypoint pages/api/graphql.ts",
		"nexus:check": "nexus build --entrypoint pages/api/graphql.ts && nexus build --entrypoint pages/api/playground.ts",
		"migrate:save": "prisma migrate save --experimental",
		"migrate:up": "prisma migrate up --experimental",
		"gen:prisma": "prisma generate",
		"migrate": "yarn migrate:save",
		"postmigrate:save": "yarn migrate:up",
		"postmigrate": "yarn gen:prisma",
		"gen:client": "graphql-codegen --config codegen.yml"
	},
	"dependencies": {
		"@apollo/client": "^3.0.2",
		"@zeit-ui/react": "^1.8.0-canary.7",
		"@zeit-ui/react-icons": "^1.3.0",
		"avataaars": "^1.2.1",
		"bcryptjs": "^2.4.3",
		"env-cmd": "^10.1.0",
		"jsonwebtoken": "^8.5.1",
		"next": "latest",
		"nexus": "^0.25.0",
		"nexus-plugin-jwt-auth": "^1.2.0",
		"nexus-plugin-paljs": "^1.1.8",
		"nexus-plugin-prisma": "^0.16.1",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"sass": "^1.26.10"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "1.17.3",
		"@graphql-codegen/typescript": "1.17.3",
		"@graphql-codegen/typescript-operations": "1.17.3",
		"@graphql-codegen/typescript-react-apollo": "1.17.3",
		"@types/bcryptjs": "^2.4.2",
		"@types/jsonwebtoken": "^8.5.0",
		"@types/node": "^14.0.24",
		"@types/react": "^16.9.43",
		"@types/react-dom": "^16.9.8",
		"typescript": "3.9.7"
	},
	"license": "ISC"
}
